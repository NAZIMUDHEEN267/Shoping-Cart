<section>
    <div class="container">
        
        <table class="table mt-5">
  <thead>
    <tr>
      <th scope="col">No.</th>
      <th scope="col">Title</th>
      <th scope="col">Category</th>
      <th scope="col">Description</th>
      <th>image</th>
    </tr>
  </thead>
  <tbody>
    {{#each products}}
    <tr>
      <th scope="row">{{this.number}}</th>
      <td>{{this.title}}</td>
      <td>{{this.mobile}}</td>
      <td>{{this.description}}</td>
      <td><img style="width: 50px;height:50px;" src="{{this.image}}" alt=""></td>
    </tr>
   {{/each}}
  </tbody>
</table>

    <div class="mt-4">
            <a href="" class="btn btn-success">Add Product</a>
        </div>

    </div>
</section>